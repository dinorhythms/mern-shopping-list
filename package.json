{
  "name": "mean-shopping-list",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "npm start --prefix client",
